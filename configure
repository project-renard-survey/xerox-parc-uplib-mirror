  withval=$with_interaction_charset; charset=$with_interaction_charset
else
  charset="UTF-8"
fi


$PYTHON -c "'a'.encode('$charset')" >/dev/null 2>&1
supported="$?"
if test "$supported" = "0"; then
  INTERACTION_CHARSET=$charset
  { echo "$as_me:$LINENO: result: $charset" >&5
echo "${ECHO_T}$charset" >&6; }
else
  { { echo "$as_me:$LINENO: error: Invalid charset encoding \"$charset\" specified.  You must use the Python name for the charset encoding." >&5
echo "$as_me: error: Invalid charset encoding \"$charset\" specified.  You must use the Python name for the charset encoding." >&2;}
   { (exit 1); exit 1; }; }
fi

