{ echo "$as_me:$LINENO: checking for $ac_word" >&5
echo $ECHO_N "checking for $ac_word... $ECHO_C" >&6; }
if test "${ac_cv_path_TIFFSPLIT+set}" = set; then
  echo $ECHO_N "(cached) $ECHO_C" >&6
else
  case $TIFFSPLIT in
  [\\/]* | ?:[\\/]*)
  ac_cv_path_TIFFSPLIT="$TIFFSPLIT" # Let the user override the test with a path.
  ;;
  *)
  as_save_IFS=$IFS; IFS=$PATH_SEPARATOR
for as_dir in $PATH
do
  IFS=$as_save_IFS
  test -z "$as_dir" && as_dir=.
  for ac_exec_ext in '' $ac_executable_extensions; do
  if { test -f "$as_dir/$ac_word$ac_exec_ext" && $as_test_x "$as_dir/$ac_word$ac_exec_ext"; }; then
    ac_cv_path_TIFFSPLIT="$as_dir/$ac_word$ac_exec_ext"
    echo "$as_me:$LINENO: found $as_dir/$ac_word$ac_exec_ext" >&5
    break 2
  fi
done
done
IFS=$as_save_IFS

  ;;
esac
fi
TIFFSPLIT=$ac_cv_path_TIFFSPLIT
if test -n "$TIFFSPLIT"; then
  { echo "$as_me:$LINENO: result: $TIFFSPLIT" >&5
echo "${ECHO_T}$TIFFSPLIT" >&6; }
else
  { echo "$as_me:$LINENO: result: no" >&5
echo "${ECHO_T}no" >&6; }
fi


