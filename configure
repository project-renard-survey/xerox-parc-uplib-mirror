if test "x$JAVAHOME" != "x"; then

  # what version of java?
  java_version=`"$JAVA" -version 2>&1 | head -1 | sed -e 's/java version //g' -e 's/"//g'`



  { echo "$as_me:$LINENO: result: looking at Java version $java_version..." >&5
echo "${ECHO_T}looking at Java version $java_version..." >&6; }
  JAVA_MAJOR_VERSION=`"$PYTHON" -c "\
import sys, re
print re.split('[\.\_]', '$java_version')[0]"`
  JAVA_MINOR_VERSION=`"$PYTHON" -c "\
import sys, re
print re.split('[\.\_]', '$java_version')[1]"`
  JAVA_MICRO_VERSION=`"$PYTHON" -c "\
import sys, re
print re.split('[\.\_]', '$java_version')[2]"`
  JAVA_BUILD_VERSION=`"$PYTHON" -c "\
import sys, re
splits=re.split('[\.\_]', '$java_version')
print ((len(splits) > 3) and splits[3]) or 5"`

  JAVA_HOTSPOT_FLAGS=""
  if test $JAVA_MAJOR_VERSION -eq 1 -a $JAVA_MINOR_VERSION -eq 6 -a $JAVA_MICRO_VERSION -eq 0 -a $JAVA_BUILD_VERSION -gt 3 -a $JAVA_BUILD_VERSION -lt 11 ; then
    JAVA_HOTSPOT_FLAGS="-Xint"
    { echo "$as_me:$LINENO: result: This version of Java ($java_version) contains a Hotspot bug which causes corrupt Lucene indices.  JIT compilation will be disabled." >&5
echo "${ECHO_T}This version of Java ($java_version) contains a Hotspot bug which causes corrupt Lucene indices.  JIT compilation will be disabled." >&6; }
  fi


  JAVA_COMPAT_FLAG=""
  JAVA_HAS_COOKIE_HANDLING=yes
  if test $JAVA_MAJOR_VERSION -ge 1 -a $JAVA_MINOR_VERSION -gt 4 ; then
    if test "$need1dot4" = "yes" ; then
      JAVA_COMPAT_FLAG="-source 1.4 -target 1.4"
      JAVA_HAS_COOKIE_HANDLING=no
      { echo "$as_me:$LINENO: result: Java version is $java_version; using JAVA_COMPAT_FLAG of \"$JAVA_COMPAT_FLAG\"" >&5
echo "${ECHO_T}Java version is $java_version; using JAVA_COMPAT_FLAG of \"$JAVA_COMPAT_FLAG\"" >&6; }
    else
      JAVA_COMPAT_FLAG="-source 1.5 -target 1.5"
      { echo "$as_me:$LINENO: result: Java version is $java_version; no JAVA_COMPAT_FLAG needed" >&5
echo "${ECHO_T}Java version is $java_version; no JAVA_COMPAT_FLAG needed" >&6; }
    fi
  elif test $JAVA_MAJOR_VERSION -eq 1 -a $JAVA_MINOR_VERSION -lt 4; then
    { { echo "$as_me:$LINENO: error: Java version is $java_version; we need at least 1.4.2." >&5
echo "$as_me: error: Java version is $java_version; we need at least 1.4.2." >&2;}
   { (exit 1); exit 1; }; }
  elif test $JAVA_MAJOR_VERSION -eq 1 -a $JAVA_MINOR_VERSION -eq 4 -a $JAVA_MICRO_VERSION -lt 2; then
    { { echo "$as_me:$LINENO: error: Java version is $java_version; we need at least 1.4.2." >&5
echo "$as_me: error: Java version is $java_version; we need at least 1.4.2." >&2;}
   { (exit 1); exit 1; }; }
  else
    if test $JAVA_MINOR_VERSION -lt 5; then
      JAVA_HAS_COOKIE_HANDLING=no
    fi
    { echo "$as_me:$LINENO: result: Java version is $java_version; no JAVA_COMPAT_FLAG needed" >&5
echo "${ECHO_T}Java version is $java_version; no JAVA_COMPAT_FLAG needed" >&6; }
  fi
  if test "$JAVA_HAS_COOKIE_HANDLING" = "no"; then
    { echo "$as_me:$LINENO: result: No cookie handling in Java clients; password must be passed on each call" >&5
echo "${ECHO_T}No cookie handling in Java clients; password must be passed on each call" >&6; }
  fi

  # check for Java debug flags
  { echo "$as_me:$LINENO: checking for Java debug" >&5
echo $ECHO_N "checking for Java debug... $ECHO_C" >&6; }
  # Check whether --enable-java-debug was given.
if test "${enable_java_debug+set}" = set; then
  enableval=$enable_java_debug; JAVADEBUGFLAGS="$enableval"
else
  JAVADEBUGFLAGS="-g"
fi


  if test "$JAVADEBUGFLAGS" = "no"; then
    { echo "$as_me:$LINENO: result: no" >&5
echo "${ECHO_T}no" >&6; }
    JAVADEBUGFLAGS=""
  elif test "$JAVADEBUGFLAGS" = "yes"; then
    JAVADEBUGFLAGS="-g"
    { echo "$as_me:$LINENO: result: yes:  $JAVADEBUGFLAGS" >&5
echo "${ECHO_T}yes:  $JAVADEBUGFLAGS" >&6; }
  else
    if test $JAVA_MAJOR_VERSION -eq 1 -a $JAVA_MINOR_VERSION -eq 4; then
      JAVADEBUGFLAGS="$JAVADEBUGFLAGS -deprecation"
    fi
    { echo "$as_me:$LINENO: result: yes:  $JAVADEBUGFLAGS" >&5
echo "${ECHO_T}yes:  $JAVADEBUGFLAGS" >&6; }
  fi

  { echo "$as_me:$LINENO: checking for Java classpath separator character" >&5
echo $ECHO_N "checking for Java classpath separator character... $ECHO_C" >&6; }

