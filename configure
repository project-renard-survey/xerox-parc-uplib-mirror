# check to see which service framework is preferred


WEB_SERVICE_FRAMEWORK=
{ echo "$as_me:$LINENO: checking which Web services framework to use" >&5
echo $ECHO_N "checking which Web services framework to use... $ECHO_C" >&6; }
if test "$python_has_tornado" != "no"; then
  # Check whether --enable-tornado was given.
if test "${enable_tornado+set}" = set; then
  enableval=$enable_tornado; v="$enableval"
else
  v="no"
fi

  if test "x$v" = "xyes"; then
    version=`$PYTHON -c "from tornado import version; print (tuple(int(x) for x in version.split('.')) >= (1, 2))"`
    if test "$version" = "True"; then
      WEB_SERVICE_FRAMEWORK="Tornado"
    else
      version=`$PYTHON -c "from tornado import version; print version"`
      { { echo "$as_me:$LINENO: error: Can't use Tornado version $version; need version 1.2 or better" >&5
echo "$as_me: error: Can't use Tornado version $version; need version 1.2 or better" >&2;}
   { (exit 1); exit 1; }; }
    fi
  fi
fi
if test "x$WEB_SERVICE_FRAMEWORK" = "x"; then
  if test "$python_has_medusa" = "yes"; then
    WEB_SERVICE_FRAMEWORK=Medusa
  else
    { { echo "$as_me:$LINENO: error: \"Need either Medusa or Tornado to run\"" >&5
echo "$as_me: error: \"Need either Medusa or Tornado to run\"" >&2;}
   { (exit 1); exit 1; }; }
  fi
fi
{ echo "$as_me:$LINENO: result: $WEB_SERVICE_FRAMEWORK" >&5
echo "${ECHO_T}$WEB_SERVICE_FRAMEWORK" >&6; }

# we need to know if stunnel is version 3 or 4

{ echo "$as_me:$LINENO: checking version of stunnel" >&5
echo $ECHO_N "checking version of stunnel... $ECHO_C" >&6; }
if test "$OS_TARGET" = "win32"; then
  STUNNEL_VERSION=4
else
  STUNNEL_VERSION_STRING4="`$STUNNEL -version 2>&1 | head -1`"
  STUNNEL_VERSION_STRING3="`$STUNNEL -V 2>&1 | head -2 | tail -1`"
  STUNNEL_VERSION=`$PYTHON -c "\
import re
m = re.match('stunnel ([0-9]+)\.([0-9]+) on.*', '$STUNNEL_VERSION_STRING3')
if not m:
   m = re.match('stunnel ([0-9]+)\.([0-9]+) on.*', '$STUNNEL_VERSION_STRING4')
if m:
   print m.group(1)
else:
 print 0
"`
fi
{ echo "$as_me:$LINENO: result: $STUNNEL_VERSION" >&5
echo "${ECHO_T}$STUNNEL_VERSION" >&6; }
if test "$STUNNEL_VERSION" = "3" ; then
  { { echo "$as_me:$LINENO: error: stunnel version 3 is no longer supported.  You must upgrade to stunnel 4 to run this version of UpLib." >&5
echo "$as_me: error: stunnel version 3 is no longer supported.  You must upgrade to stunnel 4 to run this version of UpLib." >&2;}
   { (exit 1); exit 1; }; }
fi


